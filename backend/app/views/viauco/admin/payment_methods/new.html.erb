<% content_for :page_title do %>
  <%= link_to Viauco.t(:payment_methods), viauco.admin_payment_methods_url %> /
  <%= Viauco.t(:new_payment_method) %>
<% end %>

<%= render partial: 'viauco/admin/shared/error_messages', locals: { target: @payment_method } %>

<%= form_for @payment_method, url: collection_url do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'viauco/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
