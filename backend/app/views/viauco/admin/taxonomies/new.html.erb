<% content_for :page_title do %>
  <%= link_to Viauco.t(:taxonomies), viauco.admin_taxonomies_url %> /
  <%= Viauco.t(:new_taxonomy) %>
<% end %>
<%= render partial: 'viauco/admin/shared/error_messages', locals: { target: @taxonomy } %>

<%= form_for [:admin, @taxonomy] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'viauco/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
