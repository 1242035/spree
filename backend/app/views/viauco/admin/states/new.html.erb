<%= render partial: 'viauco/admin/shared/error_messages', locals: { target: @state } %>

<% content_for :page_title do %>
  <%= link_to Viauco.t(:states), viauco.admin_country_states_url(@country) %> /
  <%= Viauco.t(:new_state) %>
<% end %>

<%= form_for [:admin, @country, @state] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'viauco/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
